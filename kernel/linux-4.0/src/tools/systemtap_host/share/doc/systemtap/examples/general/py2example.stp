probe python2.function_entry
{
    %( $# >= 1 %?
       variable = @1
       %:
       variable = "*"
       %)
    %( $# >= 2 %?
       skip_name = @2
       %:
       skip_name = ""
       %)
    python2_backtrace (skip_name)
    python2_get_variable (variable, skip_name)
}

probe python2.function_return
{
    %( $# >= 1 %?
       variable = @1
       %:
       variable = "*"
       %)
    %( $# >= 2 %?
       skip_name = @2
       %:
       skip_name = ""
       %)
    python2_get_variable (variable, skip_name)
}
